<h1>Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title, "Song Title" %>
  <%= f.text_field :title %>  

  
  <%= f.label :artist_name, "Artist Name" %>
  <%= f.text_field :artist_name %>

  
  <%= f.label "Select Category" %>
  <%= f.collection_select :genre_id, Genre.all, :name, :id %>
  
  
  <%= f.submit %>
  <% end %>
